        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
        <UserSecretsId>dotnet-ExoNet-078bdf6c-58d0-415c-b14f-1348bccc8d76</UserSecretsId>
        <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
        <Company>Alexander Semenov</Company>
